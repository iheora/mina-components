@import '../common/index.wxss';.keyboard-wrap{background-color:#f6f6f7;box-sizing:border-box;display:flex;flex-direction:row;height:auto;padding:12rpx 0 env(safe-area-inset-bottom) 12rpx;width:100vw}.keyboard-wrap .keyboard-key{background-color:#f2f3f5;box-sizing:border-box;flex:1;flex-basis:33%;padding:0 12rpx 12rpx 0;position:relative}.keyboard-wrap .keyboard-key.wider{flex-basis:66%}.keyboard-wrap .keyboard-key.higher{flex-basis:100%}.keyboard-wrap .keyboard-key .button{align-items:center;background-color:#fff;box-sizing:border-box;color:#333;display:flex;font-size:44rpx;font-weight:500;height:100rpx;justify-content:center;padding:0}.keyboard-wrap .keyboard-key .button:after{border:none}.keyboard-wrap .keyboard-key .button.pressed{background-color:#dbdbdb}.keyboard-wrap .keyboard-key .button.confirm.blue{background-color:#3b89f2;color:#fff}.keyboard-wrap .keyboard-key .button.confirm.blue.pressed{background-color:#0060e1}.keyboard-wrap .keyboard-key .button.confirm.blue.disabled{background-color:rgba(59,137,242,.64)}.keyboard-wrap .keyboard-key .button.confirm.green{background-color:#1aad19;color:#fff}.keyboard-wrap .keyboard-key .button.confirm.green.disabled,.keyboard-wrap .keyboard-key .button.confirm.green.pressed{background-color:#1aad19}.keyboard-wrap .keyboard-key .button.delete .img{height:34rpx;width:44rpx}.keyboard-wrap .keyboard-key .button.large{bottom:6px;height:auto;left:0;position:absolute;right:6px;top:0}.keyboard-wrap .keyboard-keys{display:flex;flex:3;flex-wrap:wrap}.keyboard-wrap .keyboard-sidebar{display:flex;flex:1;flex-direction:column}