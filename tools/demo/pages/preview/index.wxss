.wrap {
  padding: 0 20rpx;
}

.imgs-wrap {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.imgs-wrap .imgs-board {
  width: 168rpx;
  height: 168rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
  border: 1px solid #f5f5f5;
}

.imgs-wrap .imgs-board .img {
  width: 100%;
  height: 100%;
}

.btns-board {
  display: flex;
  justify-content:center;
  opacity: .8;
}

.btns-board .btn {
  width: 300rpx;
  height: 86rpx;
  line-height: 86rpx;
  margin-left: 30rpx;
  text-align: center;
  border-radius: 48rpx;
  font-size: 32rpx;
  border: 2rpx solid #FFFFFF;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}